<UserControl x:Class="Ebis.Tabs.JournalIncident"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Ebis.Tabs"
             mc:Ignorable="d" 
             d:DesignHeight="720" d:DesignWidth="1280">
    <StackPanel Margin="0,10,0,10">
        <TextBox x:Name="journalIncidentRecherche" TextWrapping="Wrap" Width="120" TextChanged="journalIncidentRecherche_TextChanged"/>
        <ListBox x:Name="journalIncidentList" MinHeight="100" SelectionChanged="journalIncidentList_SelectionChanged"/>
    </StackPanel>
</UserControl>
