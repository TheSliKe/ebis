<UserControl x:Class="Ebis.Tabs.JournalIncident"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Ebis.Tabs"
             mc:Ignorable="d" 
             d:DesignHeight="720" d:DesignWidth="1280">
    <StackPanel Margin="0,0,0,10">
        <Grid Height="30">
            <TextBlock Text="Recherche:" TextWrapping="Wrap" Margin="294,15,915,15" RenderTransformOrigin="0.19,0.52"/>
            <TextBox x:Name="journalIncidentRecherche" TextWrapping="Wrap" Width="540" TextChanged="JournalIncidentRecherche_TextChanged" Height="20"/>
        </Grid>
        <DataGrid x:Name="interventionDataGrid" MinHeight="100" Height="660" Margin="10,0,10,10"/>
    </StackPanel>
</UserControl>
